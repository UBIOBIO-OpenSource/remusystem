<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="sub_lib_BaseImpTOTAL" pageWidth="60" pageHeight="25" columnWidth="60" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="Mes" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Anio" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="id_empresa" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     SUM(detalle_liquidacion.`monto`) AS detalle_liquidacion_monto
FROM
     `relacion_laboral` relacion_laboral INNER JOIN `liquidacion_de_sueldo` liquidacion_de_sueldo ON relacion_laboral.`Id` = liquidacion_de_sueldo.`id_relacion_laboral`
     INNER JOIN `detalle_liquidacion` detalle_liquidacion ON liquidacion_de_sueldo.`Id` = detalle_liquidacion.`id_liquidacion_sueldo`
     INNER JOIN `empresa` empresa ON relacion_laboral.`id_empresa` = empresa.`Id`
WHERE
     liquidacion_de_sueldo.`mes` = $P{Mes}
     and liquidacion_de_sueldo.`anio` = $P{Anio}
     and detalle_liquidacion.`posicion` = 7 and empresa.`Id`=$P{id_empresa}]]>
	</queryString>
	<field name="detalle_liquidacion_monto" class="java.math.BigDecimal"/>
	<detail>
		<band height="25" splitType="Stretch">
			<textField>
				<reportElement x="0" y="0" width="60" height="25"/>
				<textElement textAlignment="Right" verticalAlignment="Middle">
					<font isBold="true"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{detalle_liquidacion_monto}.toString()]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
