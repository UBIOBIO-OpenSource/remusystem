<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="sub_liq_suelLiq" pageWidth="70" pageHeight="22" columnWidth="70" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="Mes" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="Anio" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="id_relacion" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     detalle_liquidacion.`monto` AS detalle_liquidacion_monto
FROM
     `relacion_laboral` relacion_laboral INNER JOIN `liquidacion_de_sueldo` liquidacion_de_sueldo ON relacion_laboral.`Id` = liquidacion_de_sueldo.`id_relacion_laboral`
     INNER JOIN `detalle_liquidacion` detalle_liquidacion ON liquidacion_de_sueldo.`Id` = detalle_liquidacion.`id_liquidacion_sueldo`
WHERE
     liquidacion_de_sueldo.`mes` = $P{Mes}
     and liquidacion_de_sueldo.`anio` = $P{Anio}
     and relacion_laboral.`Id` = $P{id_relacion}
     and detalle_liquidacion.`posicion` = 16]]>
	</queryString>
	<field name="detalle_liquidacion_monto" class="java.math.BigDecimal"/>
	<detail>
		<band height="20">
			<textField pattern="#,##0">
				<reportElement x="0" y="0" width="70" height="20"/>
				<box leftPadding="3" rightPadding="3">
					<bottomPen lineWidth="0.0"/>
				</box>
				<textElement textAlignment="Right" verticalAlignment="Middle" lineSpacing="Single">
					<font size="8"/>
				</textElement>
				<textFieldExpression class="java.lang.String"><![CDATA[$F{detalle_liquidacion_monto}]]></textFieldExpression>
			</textField>
		</band>
	</detail>
</jasperReport>
